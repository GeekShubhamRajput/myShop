<div id="inlineLP">
  <h1>Import Products</h1>
  <%= link_to "Sample File", sample_file_products_path(format: 'csv'), class: "btn btn-primary pull-right" %>
</div>
<br />
<br />

<%= form_tag import_products_path, multipart: true, class: 'form-inline' do %>

  <div class="form-group">
    <%= file_field_tag :file, class: '' %>
  </div>
  
  <div class="form-group">
    <%= submit_tag "Import CSV", class: 'btn btn-primary' %>
  </div>

<% end %>
